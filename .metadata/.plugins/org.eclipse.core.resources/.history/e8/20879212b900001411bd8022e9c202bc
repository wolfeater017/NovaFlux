package ai;

import Graphic.Sprite;
import tools.AI;
import tools.LM;
import tools.Mask;

public class IceCube extends AI{
	
	protected Sprite sprite;

	public IceCube(int Startx, int Starty, int Direction) {
		super(Startx, Starty, Direction);
		sprite = new Sprite(.25, .25, 1, LM.ICE_CUBE);
		mask = new Mask(0, 0, sprite.getWidth()*sprite.xscale, sprite.getHeight()*sprite.yscale);
		mask.setThing(this);
	}

}
