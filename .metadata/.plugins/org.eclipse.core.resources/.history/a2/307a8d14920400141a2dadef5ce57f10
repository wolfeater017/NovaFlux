			super(id, rect, xOffset);
			disabled = Color.gray;
			normal = Color.BLACK;
		}
		
		@Override
		public void Clicked() {
			super.Clicked();
		}
		
	}

	@Override
	public void mouseEntered(MouseEvent e) {
		
		
