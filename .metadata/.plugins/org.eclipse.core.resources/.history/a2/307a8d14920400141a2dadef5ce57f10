	private int x = CreatorApp.width*4/5, y = 35,width = CreatorApp.width/5,height = CreatorApp.height/2 -35;

	public Objects(Panel p){
		this.p = p;
		Types = p.ca.Types;
	}
	
	
	
	@Override
